{
  "name": "wildcat",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "WildCAT - CSV column reorderer with drag-and-drop interface, PWA support, and desktop app",
  "keywords": [
    "csv",
    "reorder",
    "columns",
    "drag-and-drop",
    "pwa",
    "offline",
    "react",
    "electron",
    "desktop"
  ],
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://beaux-riel.github.io/WildCAT/",
  "repository": {
    "type": "git",
    "url": "https://github.com/beaux-riel/WildCAT.git"
  },
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "cross-env ELECTRON_ENV=development vite",
    "build": "vite build",
    "build:electron": "vite build && electron-builder",
    "electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:3000 electron .",
    "electron:dev": "concurrently \"npm run dev:electron\" \"wait-on http://localhost:3000 && npm run electron\"",
    "pack": "electron-builder --dir",
    "dist": "vite build && electron-builder",
    "dist:mac": "vite build && electron-builder --mac",
    "dist:win": "vite build && electron-builder --win",
    "dist:linux": "vite build && electron-builder --linux",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.461.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.10",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-pwa": "^1.1.0",
    "wait-on": "^7.2.0",
    "concurrently": "^8.2.2",
    "workbox-window": "^7.3.0"
  }
}
